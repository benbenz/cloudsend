    "version": "0.2.0",
    "configurations": [

        {
            "name": "Demo",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/cloudsend/demo.py",
            "console": "integratedTerminal",
            "cwd": "${workspaceFolder}",
            "args": ["config.py"],
            "env": {
                "PYTHONPATH":"${workspaceRoot}:$PYTHONPATH"
            }
        },
        {
            "name": "Demo Reset",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/cloudsend/demo.py",
            "console": "integratedTerminal",
            "cwd": "${workspaceFolder}",
            "args": ["config.py","reset"],
            "env": {
                "PYTHONPATH":"${workspaceRoot}:$PYTHONPATH"
            }
        },
        {
            "name": "Demo Wait",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/cloudsend/demo.py",
            "console": "integratedTerminal",
            "cwd": "${workspaceFolder}",
            "args": ["config.py","wait"],
            "env": {
                "PYTHONPATH":"${workspaceRoot}:$PYTHONPATH"
            }
        },
        {
            "name": "Demo Montecarlo",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/cloudsend/demo.py",
            "console": "integratedTerminal",
            "cwd": "${workspaceFolder}",
            "args": ["config-montecarlo10.py","reset"],
            "env": {
                "PYTHONPATH":"${workspaceRoot}:$PYTHONPATH"
            }
        },        
        {
            "name": "Maestro Server",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/cloudsend/maestroserver.py",
            "console": "integratedTerminal",
            "cwd": "${workspaceFolder}",
            "args": ["-u"],
            "env": {
                "PYTHONPATH":"${workspaceRoot}:$PYTHONPATH"
            }
        } ,
        {
            "name": "Client Start",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/cloudsend/maestroclient.py",
            "console": "integratedTerminal",
            "cwd": "${workspaceFolder}",
            "args": ["-u","start"],
            "env": {
                "PYTHONPATH":"${workspaceRoot}:$PYTHONPATH"
            }
        }   ,
        {
            "name": "CLI Start",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/cloudsend/cli.py",
            "console": "integratedTerminal",
            "cwd": "${workspaceFolder}",
            "args": ["-u","start"],
            "env": {
                "PYTHONPATH":"${workspaceRoot}:$PYTHONPATH"
            }
